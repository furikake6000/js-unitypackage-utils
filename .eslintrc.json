{
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es2020": true,
    "jest": true
  },
  "rules": {
    "no-console": "warn"
  },
  "ignorePatterns": ["dist/", "*.test.ts", "*.test.js"]
}