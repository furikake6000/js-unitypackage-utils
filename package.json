{
  "name": "js-unitypackage-utils",
  "version": "1.0.0",
  "description": "A lightweight JavaScript/TypeScript library for manipulating Unity Package (.unitypackage) files in browsers and Node.js",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "unity",
    "unitypackage",
    "tar",
    "gz",
    "browser",
    "nodejs",
    "typescript"
  ],
  "author": "furikake6000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/furikake6000/js-unitypackage-utils.git"
  },
  "bugs": {
    "url": "https://github.com/furikake6000/js-unitypackage-utils/issues"
  },
  "homepage": "https://github.com/furikake6000/js-unitypackage-utils#readme",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "nanotar": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}